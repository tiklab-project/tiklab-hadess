    <artifactId>thoughtware-hadess</artifactId>
    <packaging>pom</packaging>
    <name>${project.artifactId}</name>
    <version>1.0.1-SNAPSHOT</version>

    <modules>
        <module>thoughtware-hadess-api</module>
        <module>thoughtware-hadess-server</module>
        <module>thoughtware-hadess-starter</module>
    </modules>

    <properties>
