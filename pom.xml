    <dependencyManagement>
        <dependencies>


        </dependencies>
    </dependencyManagement>



    <profiles>
        <profile>
            <id>deploy-hadess</id>
            <distributionManagement>
                <snapshotRepository>
                    <id>hadess</id>
                    <url>http://192.168.10.17:8081/repository/maven-snapshot</url>
                </snapshotRepository>
                <repository>
                    <id>hadess</id>
                    <url>http://192.168.10.17:8081/repository/maven-release</url>
                </repository>
            </distributionManagement>
            <properties>
                <profile.assembly.package.suffix>deploy-hadess</profile.assembly.package.suffix>
            </properties>
        </profile>

        <profile>
            <id>deploy-nexus</id>
            <distributionManagement>
                <snapshotRepository>
                    <id>nexus-public</id>
                    <url>http://172.13.1.11:8081/repository/maven-snapshots/</url>
                </snapshotRepository>
                <repository>
                    <id>nexus-public</id>
                    <url>http://172.13.1.11:8081/repository/maven-releases/</url>
                </repository>
            </distributionManagement>
            <properties>
                <profile.assembly.package.suffix>deploy-hadess</profile.assembly.package.suffix>
            </properties>
        </profile>
